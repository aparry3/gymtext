{
  "name": "gymtext",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run db:generate-types && next dev --turbopack",
    "build": "npm run db:generate-types && next build",
    "start": "next start",
    "lint": "next lint",
    "migrate": "node scripts/migrate.js",
    "migrate:create": "tsx scripts/create-migration.ts",
    "migrate:up": "tsx scripts/migrate.ts up",
    "migrate:down": "tsx scripts/migrate.ts down",
    "sms:test": "tsx scripts/test-sms.ts",
    "db:generate-types": "dotenv -e .env.local -- kysely-codegen --camel-case --out-file=src/shared/types/generated-schema.ts"
  },
  "dependencies": {
    "@dqbd/tiktoken": "^1.0.21",
    "@hookform/resolvers": "^3.3.4",
    "@langchain/core": "^0.3.45",
    "@langchain/google-genai": "^0.2.4",
    "@langchain/openai": "^0.5.6",
    "@pinecone-database/pinecone": "^5.1.1",
    "@stripe/react-stripe-js": "^2.4.0",
    "@stripe/stripe-js": "^2.4.0",
    "date-fns": "^3.3.1",
    "dotenv": "^16.3.1",
    "kysely": "^0.28.0",
    "langchain": "^0.3.23",
    "next": "15.2.4",
    "pg": "^8.14.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.50.0",
    "stripe": "^14.9.0",
    "twilio": "^5.5.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "^8.10.9",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.17",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "kysely-codegen": "^0.18.5",
    "postcss": "^8.4.35",
    "tailwindcss": "^4",
    "tsx": "^4.7.1",
    "typescript": "^5"
  }
}
