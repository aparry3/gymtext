{
  "name": "gymtext-monorepo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "dev:web": "turbo dev --filter=web",
    "dev:admin": "turbo dev --filter=admin",
    "dev:programs": "turbo dev --filter=programs",
    "build": "DATABASE_URL=$(grep '^DATABASE_URL=' .env.local | cut -d'=' -f2- | tr -d '\"') turbo build",
    "build:web": "DATABASE_URL=$(grep '^DATABASE_URL=' .env.local | cut -d'=' -f2- | tr -d '\"') turbo build --filter=web",
    "build:admin": "DATABASE_URL=$(grep '^DATABASE_URL=' .env.local | cut -d'=' -f2- | tr -d '\"') turbo build --filter=admin",
    "build:programs": "DATABASE_URL=$(grep '^DATABASE_URL=' .env.local | cut -d'=' -f2- | tr -d '\"') turbo build --filter=programs",
    "lint": "DATABASE_URL=$(grep '^DATABASE_URL=' .env.local | cut -d'=' -f2- | tr -d '\"') turbo lint",
    "inngest": "npx inngest-cli@latest dev",
    "db:codegen": "DATABASE_URL=$(grep '^DATABASE_URL=' .env.local | cut -d'=' -f2- | tr -d '\"') pnpm --filter @gymtext/shared db:codegen",
    "db:migrate": "tsx scripts/migrations/run.ts latest",
    "db:migrate:down": "tsx scripts/migrations/run.ts down",
    "db:migrate:create": "tsx scripts/migrations/create.ts",
    "migrate:create": "tsx scripts/migrations/create.ts",
    "seed:exercises": "source .env.local && npx tsx scripts/seed-exercises.ts",
    "seed:exercise-db": " npx tsx scripts/seed-exercise-db.ts",
    "seed:embeddings": "tsx scripts/seed-exercise-embeddings.ts",
    "migrate:up": "tsx scripts/migrations/run.ts up",
    "migrate:latest": "tsx scripts/migrations/run.ts latest",
    "migrate:down": "tsx scripts/migrations/run.ts down",
    "db:dump": "tsx scripts/db/dump-data.ts",
    "db:restore": "tsx scripts/db/restore-data.ts",
    "db:reset-migrations": "tsx scripts/db/reset-migration-history.ts",
    "db:anonymize": "source .env.local && tsx scripts/db/anonymize-prod-to-dev.ts",
    "db:anonymize:dry": "source .env.local && tsx scripts/db/anonymize-prod-to-dev.ts --dry-run",
    "sms:test": "source .env.local && tsx scripts/sms-test.ts",
    "favicon:generate": "node scripts/generate-favicons.js",
    "backup:local": "source .env.local && ./scripts/backup-db.sh",
    "backup:prod": "source .env.production && ./scripts/backup-db.sh",
    "migrate:training": "tsx scripts/migrations/regenerate-training-data.ts",
    "migrate:training:dry": "tsx scripts/migrations/regenerate-training-data.ts --dry-run",
    "agent:upsert": "source .env.local && tsx scripts/agent-definition.ts upsert",
    "agent:get": "source .env.local && tsx scripts/agent-definition.ts get",
    "agent:list": "source .env.local && tsx scripts/agent-definition.ts list",
    "agent:update-prompts": "source .env.local && tsx scripts/update-agent-prompts.ts",
    "seed:templates": "source .env.local && npx tsx scripts/seed-context-templates.ts"
  },
  "dependencies": {
    "@codemirror/commands": "^6.10.1",
    "@codemirror/lang-markdown": "^6.5.0",
    "@codemirror/language": "^6.12.0",
    "@codemirror/state": "^6.5.2",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "^6.39.5",
    "@dqbd/tiktoken": "^1.0.21",
    "@google/genai": "^1.31.0",
    "@hookform/resolvers": "^3.3.4",
    "@langchain/core": "^1.1.3",
    "@langchain/google-genai": "^0.2.4",
    "@langchain/openai": "^0.6.13",
    "@pinecone-database/pinecone": "^5.1.1",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-tabs": "^1.1.13",
    "@stripe/react-stripe-js": "^2.4.0",
    "@stripe/stripe-js": "^2.4.0",
    "@types/luxon": "^3.7.0",
    "@types/uuid": "^10.0.0",
    "@vercel/blob": "^2.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.1",
    "dotenv": "^16.3.1",
    "inngest": "^3.44.2",
    "kysely": "^0.28.0",
    "langchain": "^0.3.23",
    "lucide-react": "^0.331.0",
    "luxon": "^3.7.1",
    "next": "16.0.7",
    "openai": "^4.95.1",
    "pg": "^8.14.1",
    "react": "^19.0.0",
    "react-day-picker": "^9.13.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.50.0",
    "react-markdown": "^10.1.0",
    "redis": "^5.6.0",
    "remark-gfm": "^4.0.1",
    "stripe": "^14.9.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "twilio": "^5.5.2",
    "uuid": "^11.1.0",
    "zod": "^3.22.4"
  },
  "pnpm": {
    "overrides": {
      "zod": "3.25.67"
    }
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.0",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^20",
    "@types/pg": "^8.10.9",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/table": "^6.3.2",
    "autoprefixer": "^10.4.17",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "inquirer": "^12.9.2",
    "kysely-codegen": "^0.18.5",
    "postcss": "^8.4.35",
    "sharp": "^0.34.3",
    "table": "^6.9.0",
    "tailwindcss": "^4",
    "tsx": "^4.7.1",
    "turbo": "^2.3.3",
    "typescript": "^5"
  },
  "packageManager": "pnpm@9.15.0"
}
