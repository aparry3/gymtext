#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/aaronparry/Developer/GymText/gymtext/node_modules/.pnpm/kysely-codegen@0.18.5_kysely@0.28.0_pg@8.14.1_typescript@5.8.3/node_modules/kysely-codegen/dist/cli/node_modules:/Users/aaronparry/Developer/GymText/gymtext/node_modules/.pnpm/kysely-codegen@0.18.5_kysely@0.28.0_pg@8.14.1_typescript@5.8.3/node_modules/kysely-codegen/dist/node_modules:/Users/aaronparry/Developer/GymText/gymtext/node_modules/.pnpm/kysely-codegen@0.18.5_kysely@0.28.0_pg@8.14.1_typescript@5.8.3/node_modules/kysely-codegen/node_modules:/Users/aaronparry/Developer/GymText/gymtext/node_modules/.pnpm/kysely-codegen@0.18.5_kysely@0.28.0_pg@8.14.1_typescript@5.8.3/node_modules:/Users/aaronparry/Developer/GymText/gymtext/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/aaronparry/Developer/GymText/gymtext/node_modules/.pnpm/kysely-codegen@0.18.5_kysely@0.28.0_pg@8.14.1_typescript@5.8.3/node_modules/kysely-codegen/dist/cli/node_modules:/Users/aaronparry/Developer/GymText/gymtext/node_modules/.pnpm/kysely-codegen@0.18.5_kysely@0.28.0_pg@8.14.1_typescript@5.8.3/node_modules/kysely-codegen/dist/node_modules:/Users/aaronparry/Developer/GymText/gymtext/node_modules/.pnpm/kysely-codegen@0.18.5_kysely@0.28.0_pg@8.14.1_typescript@5.8.3/node_modules/kysely-codegen/node_modules:/Users/aaronparry/Developer/GymText/gymtext/node_modules/.pnpm/kysely-codegen@0.18.5_kysely@0.28.0_pg@8.14.1_typescript@5.8.3/node_modules:/Users/aaronparry/Developer/GymText/gymtext/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../kysely-codegen/dist/cli/bin.js" "$@"
else
  exec node  "$basedir/../kysely-codegen/dist/cli/bin.js" "$@"
fi
