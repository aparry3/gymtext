{
  "name": "@gymtext/shared",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./server": {
      "types": "./src/server/index.ts",
      "import": "./src/server/index.ts"
    },
    "./shared": {
      "types": "./src/shared/index.ts",
      "import": "./src/shared/index.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "db:codegen": "kysely-codegen --dialect postgres --camel-case --out-file ./src/server/models/_types/index.ts --exclude-pattern '^kysely_' --log-level info --type-only-imports"
  },
  "dependencies": {
    "@dqbd/tiktoken": "^1.0.21",
    "@google/genai": "^1.31.0",
    "@langchain/core": "^1.1.3",
    "@langchain/google-genai": "^0.2.4",
    "@langchain/openai": "^0.6.13",
    "@pinecone-database/pinecone": "^5.1.1",
    "@vercel/blob": "^2.0.0",
    "inngest": "^3.44.2",
    "kysely": "^0.28.0",
    "langchain": "^0.3.23",
    "luxon": "^3.7.1",
    "openai": "^4.95.1",
    "papaparse": "^5.5.3",
    "pdfjs-dist": "^4.10.38",
    "pg": "^8.14.1",
    "redis": "^5.6.0",
    "stripe": "^14.9.0",
    "twilio": "^5.5.2",
    "unpdf": "^1.4.0",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.25.1"
  },
  "devDependencies": {
    "@types/luxon": "^3.7.0",
    "@types/papaparse": "^5.5.2",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^10.0.0",
    "kysely-codegen": "^0.18.5",
    "typescript": "^5"
  },
  "peerDependencies": {
    "next": ">=14.0.0"
  }
}
